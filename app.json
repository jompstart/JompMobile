{
  "expo": {
    "scheme": "jompstart",
    "name": "JompStart",
    "slug": "JompStart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pngs/appLogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jomp.jompstart",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app requires camera access to enable you to upload your documents and record videos.",
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos."
      }
    },
    "splash": {
      "image": "./assets/images/pngs/appLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.jomp.jompstart",
      "permissions": [
        "android.permission.CAMERA_ROLL",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "com.google.android.gms.permission.AD_ID",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
 
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/AvenirLTStd-Black.otf",
            "./assets/fonts/AvenirLTStd-BlackOblique.otf",
            "./assets/fonts/AvenirLTStd-Book.otf",
            "./assets/fonts/AvenirLTStd-BookOblique.otf",
            "./assets/fonts/AvenirLTStd-Heavy.otf",
            "./assets/fonts/AvenirLTStd-HeavyOblique.otf",
            "./assets/fonts/AvenirLTStd-Light.otf",
            "./assets/fonts/AvenirLTStd-LightOblique.otf",
            "./assets/fonts/AvenirLTStd-Medium.otf",
            "./assets/fonts/AvenirLTStd-MediumOblique.otf",
            "./assets/fonts/AvenirLTStd-Roman.otf",
            "./assets/fonts/AvenirLTStd-Oblique.otf",
            "./assets/fonts/AvenirLTStd-Black.otf",
            "./assets/fonts/AvenirLTStd-BlackOblique.otf",
            "./assets/fonts/AvenirLTStd-Book.otf",
            "./assets/fonts/AvenirLTStd-BookOblique.otf",
            "./assets/fonts/AvenirLTStd-Heavy.otf",
            "./assets/fonts/AvenirLTStd-HeavyOblique.otf",
            "./assets/fonts/AvenirLTStd-Light.otf",
            "./assets/fonts/AvenirLTStd-LightOblique.otf",
            "./assets/fonts/AvenirLTStd-Medium.otf",
            "./assets/fonts/AvenirLTStd-MediumOblique.otf",
            "./assets/fonts/AvenirLTStd-Roman.otf",
            "./assets/fonts/AvenirLTStd-Oblique.otf"
          ]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app requires camera access to enable you to upload your documents."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "NSCameraUsageDescription": "This app requires camera access to enable you to upload your documents. We respect your privacy and do not access your camera without your explicit permission.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "beff92a5-f32e-49f1-a5fa-e616f0e8b13e"
      }
    }
  }
}
